#include <iostream>
using namespace std;
int main()
{
    int num1,num2;
    cout<<"ingrese el primer numero entero";
    cin>>num1;
    cout<<"ingrese el segundo numero entero";
    cin>>num2;
    if(num1>num2){
        cout<<"el primer numero es mayor\n";
    }else if(num1<num2){
        cout<<"el segundo numero es mayor";
    }else{
        cout<<"los numeros son iguales";
    }

    return 0;
}
